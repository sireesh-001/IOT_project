    delay(1000);
    digitalWrite (led_pin, LOW);
    Message = "Motion Detected";
    String data = "{\"Activity\":\""+ String(Message) +"\"}";
    server.send(200, "text/plane", data);                
    Serial.println("Motion detected!");
    }
}  
